<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.DatePicker?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="576.0" prefWidth="783.0" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="baseline.ApplicationsController">
   <children>
      <Button fx:id="addTodoListButton" layoutX="464.0" layoutY="30.0" mnemonicParsing="false" onAction="#clickAddTodoList" text="Add" />
      <DatePicker fx:id="date" layoutX="20.0" layoutY="31.0" onAction="#typedMonth" promptText="MM//DD//YYYY" />
      <TextField fx:id="description" alignment="CENTER" layoutX="208.0" layoutY="30.0" onAction="#typedDescription" prefHeight="26.0" prefWidth="248.0" promptText="Description " />
      <TableView layoutX="-3.0" layoutY="62.0" prefHeight="618.0" prefWidth="927.0">
        <columns>
          <TableColumn fx:id="tableDate" prefWidth="150.39996337890625" text="Date" />
            <TableColumn fx:id="tableDescription" prefWidth="686.3999938964844" text="Description" />
            <TableColumn fx:id="tableComplete" minWidth="0.0" prefWidth="89.60002441406255" text="Complete" />
        </columns>
      </TableView>
      <Button fx:id="removeButton" layoutX="515.0" layoutY="30.0" mnemonicParsing="false" onAction="#removeClicked" text="Remove" />
      <TextField fx:id="newTitle" alignment="TOP_CENTER" layoutX="381.0" layoutY="1.0" onAction="#typedTitle" promptText="Edit Name of Title" />
      <Button fx:id="buttonSaveName" layoutX="524.0" layoutY="1.0" mnemonicParsing="false" onAction="#clickedSaveName" prefHeight="15.0" prefWidth="85.0" text="Save Name" />
      <TextField fx:id="removeNumber" layoutX="575.0" layoutY="30.0" onAction="#typedRemoveNum" prefHeight="26.0" prefWidth="46.0" promptText="Num" />
      <Button fx:id="saveButton" layoutX="37.0" layoutY="1.0" mnemonicParsing="false" onAction="#clickedSaveFile" text="Save" />
      <Button fx:id="newButton" layoutX="78.0" layoutY="1.0" mnemonicParsing="false" onAction="#newFileClicked" text="New" />
      <Button fx:id="openButton" layoutX="118.0" layoutY="1.0" mnemonicParsing="false" onAction="#openFileClicked" text="Open" />
      <Button fx:id="showCompletedButton" layoutX="691.0" layoutY="30.0" mnemonicParsing="false" onAction="#CompletedButtonClicked" text="Show Completed" />
      <Button fx:id="showAllButton" layoutX="629.0" layoutY="30.0" mnemonicParsing="false" onAction="#showAllClicked" text="Show All" />
      <Button fx:id="showUncompletedButton" layoutX="798.0" layoutY="30.0" mnemonicParsing="false" onAction="#clickedShowUncompleted" text="Show Uncompleted" />
   </children>
</AnchorPane>
